    "vite": "^3.0.8",
    "vite-plugin-electron": "^0.9.3",
    "vite-plugin-vuetify": "^1.0.0-alpha.17",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^0.40.1"
