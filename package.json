{
  "name": "rona-tools",
  "version": "1.0.0",
  "main": "dist/electron/main/index.js",
  "description": "Electron + Vue + Vite app.",
  "author": "Michael Cavanaugh",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder"
  },
  "engines": {
